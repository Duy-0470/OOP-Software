<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:s="clr-namespace:CamDo.UC.SwitchViewClass"
                    xmlns:v="clr-namespace:CamDo.UC.UCView"> 

    <DataTemplate DataType="{x:Type s:SwitchViewCustomer }">
        <v:CustomerWindow/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type s:SwitchViewExpense }">
        <v:ExpenseWindow/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type s:SwitchViewSearch }">
        <v:SearchWindow/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type s:SwitchViewHome }">
        <v:HomeWindow/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type s:SwitchViewManagement }">
        <v:ManagementWindow/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type s:SwitchViewRemind }">
        <v:RemindWindow/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type s:SwitchViewPawn }">
        <v:PawnWindow/>
    </DataTemplate>
    
</ResourceDictionary>